{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "site": "rusty-operations-community-website",
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "/news",
        "destination": "/news.html"
      },
      {
        "source": "/news/view",
        "destination": "/news/view.html"
      },
      {
        "source": "/news/service-alerts",
        "destination": "/news/service-alerts.html"
      },
      {
        "source": "/forums",
        "destination": "/forums.html"
      },
      {
        "source": "/forums/view",
        "destination": "/forums/view.html"
      }
    ]
  }
}
